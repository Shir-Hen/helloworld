pipeline {
    agent any
    tools { 
        maven 'maven-3.6.3'
    }
    stages {
        stage ('Initialize') {
            steps {
                    echo "Init"
            }
        }
    node ("windows") {
        stage ('Build') {
            steps {
                sh 'mvn clean package'
                echo 'Shir is finishing Building her Project'
            }
        }
      }
    }
}
