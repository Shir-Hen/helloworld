pipeline {
    agent any
    tools { 
        maven 'maven-3.6.3'
    }
    stages {
        stage ('Initialize') {
            steps {
                    echo "Init"
            }
        }
        stage ('Build') {
            steps {
                bat 'mvn clean package'
                echo 'Shir finished Building her Project'
            }
      }
      stage ('Docker Build') {
            steps {
               // bat 'java -1 jar target/multi-module-application-2.0.0.jar'//
				  bat 'docker build . -t shir-docker-app -f docker/Dockerfile'
                echo 'Shir is finishing Running her Project'  
            }
      }      

        
    }
}
